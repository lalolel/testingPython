    "print(\"\\n=== Special Power Functions ===\")\n",
    "\n",
    "# Demonstrate special cases and utilities\n",
    "values = [(3, 4), (5, 12), (8, 15), (1, 1)]\n",
    "\n",
    "print(\"Pythagorean theorem using math.hypot():\")\n",
    "for a, b in values:\n",
    "    # math.hypot(a, b) = sqrt(a² + b²) but more numerically stable\n",
    "    hypot_result = math.hypot(a, b)\n",
