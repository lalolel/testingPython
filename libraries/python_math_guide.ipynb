    "\n",
    "### Gamma Function and Error Function"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\"=== Gamma Function ===\")\n",
    "# The gamma function is a generalization of factorial: Γ(n) = (n-1)!\n",
    "\n",
    "gamma_values = [1, 2, 3, 4, 5, 0.5, 1.5, 2.5]\n",
    "\n",
    "print(f\"{'x':<6} {'Γ(x)':<12} {'Relationship':<25}\")\n",
    "print(\"-\" * 45)\n",
    "\n",
    "for x in gamma_values:\n",
    "    gamma_val = math.gamma(x)\n",
    "    \n",
    "    if x == int(x) and x > 0:\n",
    "        relationship = f\"({int(x)-1})! = {math.factorial(int(x)-1)}\"\n",
    "    elif x == 0.5:\n",
    "        relationship = f\"√π = {math.sqrt(math.pi):.6f}\"\n",
    "    else:\n",
    "        relationship = \"\"\n",
    "    \n",
    "    print(f\"{x:<6} {gamma_val:<12.6f} {relationship:<25}\")\n",
    "\n",
    "print(\"\\n=== Log Gamma Function ===\")\n",
    "# For large numbers, log(gamma(x)) is more stable than gamma(x)\n",
    "\n",
    "large_values = [10, 50, 100, 500]\n",
    "print(f\"{'x':<6} {'lgamma(x)':<15} {'Equivalent to':<20}\")\n",
    "print(\"-\" * 45)\n",
    "\n",
    "for x in large_values:\n",
    "    lgamma_val = math.lgamma(x)\n",
    "    equivalent = f\"log({x-1}!)\"\n",
    "    print(f\"{x:<6} {lgamma_val:<15.6f} {equivalent:<20}\")\n",
    "\n",
    "print(\"\\n=== Error Function ===\")\n",
    "# The error function is important in statistics and probability\n",
