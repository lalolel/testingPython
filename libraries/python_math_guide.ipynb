    "## 2. Basic Mathematical Operations {#basic-operations}\n",
    "\n",
    "### Ceiling, Floor, and Truncation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import math\n",
    "\n",
    "# Test values\n",
    "positive_float = 4.7\n",
    "negative_float = -4.7\n",
    "\n",
    "print(\"=== Ceiling, Floor, and Truncation ===\")\n",
    "print(f\"Original values: {positive_float}, {negative_float}\")\n",
    "print()\n",
    "\n",
    "# Ceiling - smallest integer >= x\n",
    "print(f\"math.ceil({positive_float}) = {math.ceil(positive_float)}\")\n",
    "print(f\"math.ceil({negative_float}) = {math.ceil(negative_float)}\")\n",
    "print()\n",
    "\n",
    "# Floor - largest integer <= x\n",
    "print(f\"math.floor({positive_float}) = {math.floor(positive_float)}\")\n",
    "print(f\"math.floor({negative_float}) = {math.floor(negative_float)}\")\n",
    "print()\n",
    "\n",
    "# Truncation - remove fractional part\n",
    "print(f\"math.trunc({positive_float}) = {math.trunc(positive_float)}\")\n",
    "print(f\"math.trunc({negative_float}) = {math.trunc(negative_float)}\")\n",
    "print()\n",
    "\n",
    "# Fractional and integer parts\n",
    "frac_pos, int_pos = math.modf(positive_float)\n",
    "frac_neg, int_neg = math.modf(negative_float)\n",
    "print(f\"math.modf({positive_float}) = fractional: {frac_pos}, integer: {int_pos}\")\n",
    "print(f\"math.modf({negative_float}) = fractional: {frac_neg}, integer: {int_neg}\")"
   ]
  },
