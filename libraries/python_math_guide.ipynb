    "## 6. Number Theory Functions {#number-theory}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\"=== Factorial and Combinatorics ===\")\n",
    "\n",
    "# Factorial function\n",
    "factorial_values = [0, 1, 2, 3, 4, 5, 10, 15]\n",
    "\n",
    "print(\"Factorials:\")\n",
    "for n in factorial_values:\n",
    "    if n <= 15:  # Avoid very large numbers for display\n",
    "        fact = math.factorial(n)\n",
    "        print(f\"{n}! = {fact:,}\")\n",
    "\n",
    "print(\"\\n=== GCD and LCM Functions ===\")\n",
    "\n",
    "# Greatest Common Divisor\n",
    "gcd_pairs = [(12, 8), (15, 25), (17, 19), (48, 18), (100, 75)]\n",
    "\n",
    "print(\"GCD examples:\")\n",
    "for a, b in gcd_pairs:\n",
    "    gcd_val = math.gcd(a, b)\n",
    "    print(f\"gcd({a}, {b}) = {gcd_val}\")\n",
    "\n",
    "# LCM can be calculated using: lcm(a,b) = abs(a*b) // gcd(a,b)\n",
    "print(\"\\nLCM examples (calculated using GCD):\")\n",
    "for a, b in gcd_pairs:\n",
    "    gcd_val = math.gcd(a, b)\n",
    "    lcm_val = abs(a * b) // gcd_val\n",
    "    print(f\"lcm({a}, {b}) = {lcm_val}\")\n",
    "\n",
    "print(\"\\n=== Combinatorics Functions ===\")\n",
    "\n",
    "# Combinations and permutations\n",
    "print(\"Combinations C(n,k) = n! / (k! * (n-k)!):\")\n",
    "comb_examples = [(5, 2), (10, 3), (8, 0), (8, 8), (20, 5)]\n",
    "\n",
    "for n, k in comb_examples:\n",
    "    if hasattr(math, 'comb'):  # Available in Python 3.8+\n",
    "        comb_val = math.comb(n, k)\n",
    "    else:\n",
    "        # Manual calculation for older Python versions\n",
    "        comb_val = math.factorial(n) // (math.factorial(k) * math.factorial(n - k))\n",
    "    print(f\"C({n},{k}) = {comb_val}\")\n",
    "\n",
    "print(\"\\nPermutations P(n,k) = n! / (n-k)!:\")\n",
    "for n, k in comb_examples:\n",
    "    if hasattr(math, 'perm'):  # Available in Python 3.8+\n",
    "        perm_val = math.perm(n, k)\n",
    "    else:\n",
    "        # Manual calculation\n",
    "        perm_val = math.factorial(n) // math.factorial(n - k)\n",
    "    print(f\"P({n},{k}) = {perm_val}\")"
   ]
