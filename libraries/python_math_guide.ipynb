    "    elif exp == 1/3:\n",
    "        note = \"Cube root\"\n",
    "    elif exp < 0:\n",
    "        note = \"Reciprocal\"\n",
    "    else:\n",
    "        note = \"\"\n",
    "    \n",
    "    print(f\"{exp:<10} {pow_result:<12.4f} {builtin_result:<12.4f} {note:<20}\")\n",
